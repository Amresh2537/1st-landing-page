{
    "version": 2,
    "functions": {
      "api/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/send-email",
        "dest": "/api/send-email.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  